{
  "sprites": {
  {{#each sprites}}
    "{{name}}": {
      "width": {{width}},
      "height": {{height}}
    }{{^last}},{{/last}}
  {{/each}}
  },
  "tiles": {
  {{#each tiles}}
    "{{name}}": {
      "width": {{width}},
      "height": {{height}},
      "x": {{x}},
      "y": {{y}},
      "sprite": "{{sprite}}"
    }{{^last}},{{/last}}
  {{/each}}
  }
}
